{
  "manifest_version": 3,
  "name": "Manga Reader",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
     "16": "images.png" 
    }
  },
  "permissions": ["activeTab", "contextMenus", "scripting"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["page-eater.js"]
    }
  ]
}
